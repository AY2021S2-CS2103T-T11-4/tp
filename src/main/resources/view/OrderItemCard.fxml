<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <padding>
        <Insets top="5" right="5" bottom="5" left="5" />
    </padding>
    <HBox>
        <HBox>
            <Label fx:id="index" text="\$first" styleClass="cell_small_label" wrapText="true" prefWidth="50"/>
        </HBox>
        <HBox>
            <Label fx:id="type" text="\$first" styleClass="cell_small_label" wrapText="true" prefWidth="250"/>
        </HBox>
    </HBox>
</GridPane>

